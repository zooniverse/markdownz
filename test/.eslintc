{
  "rules": {
    "func-names": 0,
    "prefer-arrow-callback": 0,
    "import/prefer-default-export": 0,
    "max-len": 0,
    "react/prop-types": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-unused-expressions": 0,
    "space-before-function-paren": 0,
  }
}